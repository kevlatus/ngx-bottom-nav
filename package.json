{
  "name": "bottom-nav-app",
  "version": "0.0.0",
  "scripts": {
    "compodoc": "npx compodoc --disablePrivateOrInternalSupport -p projects/bottom-nav/tsconfig.lib.json -n \"NGX Bottom Nav Docs\" -d docs --theme material",
    "start": "ng serve",
    "build": "ng build bottom-nav && npm run build:scss",
    "build:scss": "scss-bundle -c scss-bundle.config.json",
    "test": "ng test bottom-nav",
    "lint": "ng lint bottom-nav",
    "e2e": "ng e2e bottom-nav",
    "publish": "npm run build && node publish.js",
    "watch": "ng build --watch bottom-nav"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
