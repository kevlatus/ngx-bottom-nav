{
  "name": "bottom-nav-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "watch": "ng build --project=bottom-nav --watch",
    "build": "ng build --project=bottom-nav --prod && npm run build:scss && npm run copy-files",
    "build:scss": "scss-bundle -p scss-bundle.config.json",
    "test": "ng test --project=bottom-nav",
    "lint": "ng lint --project=bottom-nav",
    "copy-files": "cpx LICENSE dist/bottom-nav && cpx README.md dist/bottom-nav"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.2",
    "@angular-devkit/build-ng-packagr": "^0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/jasmine": "~3.5.4",
    "@types/jasminewd2": "~2.0.7",
    "@types/node": "^13.7.1",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "scss-bundle": "^3.0.2",
    "ts-node": "~8.6.2",
    "tslib": "^1.9.0",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  }
}
